{
	"name": "@snippetbox/web",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier . --write --experimental-cli --cache --cache-location=.prettiercache",
		"lint": "prettier . --check --experimental-cli --cache --cache-location=.prettiercache && oxlint --type-aware .",
		"up:deps": "pnpm update -i --latest",
		"preinstall": "npx only-allow pnpm"
	},
	"dependencies": {
		"ky": "^1.14.1",
		"valibot": "^1.2.0"
	},
	"devDependencies": {
		"@iconify-json/lucide": "^1.2.81",
		"@prettier/plugin-oxc": "^0.1.3",
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/kit": "^2.49.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@types/node": "^25.0.3",
		"@unocss/preset-icons": "^66.5.10",
		"oxlint": "^1.34.0",
		"oxlint-tsgolint": "^0.9.2",
		"prettier": "^3.7.4",
		"prettier-plugin-svelte": "^3.4.1",
		"svelte": "^5.46.0",
		"svelte-check": "^4.3.4",
		"typescript": "^5.9.3",
		"unocss": "^66.5.10",
		"vite": "^7.3.0"
	},
	"engines": {
		"node": "24.12.0"
	},
	"packageManager": "pnpm@10.26.1"
}
