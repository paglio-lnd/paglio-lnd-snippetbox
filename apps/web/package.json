{
	"name": "@snippetbox/web",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier . --write --experimental-cli --cache --cache-location=.prettiercache",
		"lint": "prettier . --check --experimental-cli --cache --cache-location=.prettiercache && oxlint --type-aware .",
		"up:deps": "pnpm update -i --latest",
		"preinstall": "npx only-allow pnpm"
	},
	"devDependencies": {
		"@iconify-json/lucide": "^1.2.71",
		"@prettier/plugin-oxc": "^0.0.4",
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/kit": "^2.48.3",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@types/node": "^24.9.2",
		"@unocss/preset-icons": "^66.5.4",
		"globals": "^16.0.0",
		"oxlint": "^1.25.0",
		"oxlint-tsgolint": "^0.4.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"svelte": "^5.43.1",
		"svelte-check": "^4.3.3",
		"typescript": "^5.9.3",
		"unocss": "^66.5.4",
		"vite": "^7.1.12"
	},
	"engines": {
		"node": "24.11.0"
	},
	"packageManager": "pnpm@10.20.0",
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		]
	},
	"dependencies": {
		"valibot": "^1.1.0"
	}
}
